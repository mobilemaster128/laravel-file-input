{
  "name": "mobilemaster/laravel-file-input",
  "description": "Laravel 5/File Input - Handle large file uploads",
  "authors": [
    {
      "name": "Mobile Master",
      "email": "mobilemaster128@gmail.com"
    }
  ],
  "keywords": ["laravel", "file-input", "large files"],
  "license": "MIT",
  "extra": {
    "laravel": {
      "providers": [
          "MobileMaster\\LaravelFileInput\\ServiceProvider"
      ],
      "aliases": {
          "FileInput": "MobileMaster\\LaravelFileInput\\Facades\\FileInput"
    }
    }
  },
  "require": {
    "php": ">=5.5.0",
